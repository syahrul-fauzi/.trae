{
  "name": "sba-module-boilerplate",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "tsx src/index.ts",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/index.js",
    "lint": "eslint .",
    "test": "jest",
    "docker:build": "docker build -t sba-module:latest .",
    "docker:run": "docker run --env-file .env -p 3000:3000 sba-module:latest"
  },
  "dependencies": {
    "fastify": "^4.27.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "eslint": "^9.12.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "tsx": "^4.19.0",
    "typescript": "^5.6.3"
  }
}
